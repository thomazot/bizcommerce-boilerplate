.banner--nojs img.m_img
    display: none

.banner--nojs .banner__item
    display: block
    overflow: hidden
    break-inside: avoid-column
    page-break-inside: avoid

.banner:not(.banner--nojs) img.d_img
    position: absolute
    top: 50%
    left: 50%
    display: block
    max-width: none
    width: auto
    height: 100%
    transform: translate(-50%, -50%)

.banner:not(.banner--nojs) img.m_img
