.owl-prev
    right: 100%

.owl-page span
    background: var(--primary)

/* Feed RSS */
.feeds__list
    display: flex
    flex-flow: row wrap
    margin: 0 -1rem -2rem

.feeds__post
    display: flex
    flex: 1 1 33.333%
    padding: 0 1rem 2rem
